@model List<WalutyBusinessLogic.LoadingFromFile.Currency>

@{
    ViewData["Title"] = "Index";
}

<h1>Favorite currencies</h1>



    <table class="table">
        <thead>
        <tr>
            <th scope="col">Currency Code</th>
        </tr>
        <tr></tr>
        </thead>
        <tbody>
        @foreach (var pair in Model)
        {
            <tr>
                <td>@pair.Name</td>
            </tr>
        }
        </tbody>
    </table>
}

